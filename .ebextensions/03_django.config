container_commands:
  01_makemigrations:
    command: "python manage.py makemigrations"
  02_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
  03_createsu:
    command: "python manage.py createsu"
    leader_only: true
  04_collectstatic:
    command: "python manage.py collectstatic --noinput"